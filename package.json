{
  "name": "@ismaelmartinez/generator-atlassian-compass-event-catalog",
  "version": "0.2.2",
  "description": "Atlassian Compass generator for Event Catalog",
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write .",
    "format:diff": "prettier --list-different .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "changeset": "changeset",
    "release": "changeset publish"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "atlassian",
    "compass",
    "EventCatalog",
    "generator"
  ],
  "author": "ismaelmartinez@gmail.com",
  "packageManager": "pnpm@9.15.9",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.2",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-v8": "^4.0.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^4.0.0",
    "@changesets/cli": "^2.29.8"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@eventcatalog/sdk": "^2.12.0",
    "chalk": "^5.6.2",
    "change-case": "^5.4.4",
    "js-yaml": "^4.1.0",
    "zod": "^4.3.6"
  }
}
